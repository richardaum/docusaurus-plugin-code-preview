{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "docusaurus-plugin-code-preview",
  "author": "Sean Perkins",
  "module": "dist/docusaurus-plugin-code-preview.esm.js",
  "size-limit": [
    {
      "path": "dist/docusaurus-plugin-code-preview.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/docusaurus-plugin-code-preview.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@docusaurus/types": "2.0.0-beta.18",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "clsx": "^1.1.1",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.18",
    "@docusaurus/theme-classic": "2.0.0-beta.18",
    "@stackblitz/sdk": "^1.6.0",
    "@tippyjs/react": "^4.2.6"
  }
}
